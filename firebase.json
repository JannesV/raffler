{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [],
    "redirects": [
      {
        "source": "/api/**",
        "destination": "https://thingproxy.freeboard.io/",
        "type": 301
      }
    ],
    "headers": [
      {
        "source": "/api*",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }
}
